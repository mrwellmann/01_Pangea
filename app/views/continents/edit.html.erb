<h1>Editing continent</h1>

<% form_for(@continent) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :continent_name %><br />
    <%= f.text_field :continent_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @continent %> |
<%= link_to 'Back', continents_path %>